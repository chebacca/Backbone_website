{
  "name": "server",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "nodemon --exec \"npx tsx src/index.ts\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "test:invoices": "echo 'invoice tests disabled (Firestore migration)' && exit 0",
    "seed": "npx tsx scripts/seed-database.ts seed",
    "seed:clear": "npx tsx scripts/seed-database.ts clear",
    "seed:reset": "npx tsx scripts/seed-database.ts reset",
    "seed:enterprise-admin": "npx tsx scripts/seed-enterprise-admin.ts",
    "seed:all": "npx tsx scripts/seed-all.ts",
    "seed:enterprise-multi": "npx tsx scripts/seed-enterprise-multi.ts",
    "db:init": "npx tsx scripts/init-database.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:seed": "prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "@sendgrid/mail": "^8.1.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.4.0",
    "google-auth-library": "^9.14.2",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "stripe": "^14.14.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.5",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/uuid": "^9.0.8",
    "nodemon": "^3.1.10",
    "prisma": "^6.3.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "tsx scripts/seed-database.ts seed"
